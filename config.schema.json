{
	"pluginAlias": "SolarEdge Inverter",
	"pluginType": "accessory",
	"schema": {
		"name": {
			"title": "Name",
			"type": "string",
			"required": true,
			"default": "SolarEdge Inverter"
		},
		"manufacturer": {
			"title": "Manufacturer",
			"type": "string",
			"required": false,
			"default": ""
		},
		"model": {
			"title": "Inverter Model",
			"type": "string",
			"required": false,
			"default": ""
		},
		"serial": {
			"title": "Serial Number",
			"type": "string",
			"required": false,
			"default": ""
		},
		"site_id": {
			"title": "Site ID",
			"type": "string",
			"required": true,
			"default": ""
		},
		"api_key": {
			"title": "API KeyD",
			"type": "string",
			"required": true,
			"default": ""
		},
		"display": {
			"type": "array",
			"items": {
				"title": "Display Config",
				"type": "object",
				"properties": {
					"current": {
						"title": "Current Power",
						"type": "boolean",
						"default": true
					},
					"last_day": {
						"title": "Current Day's Power",
						"type": "boolean",
						"default": false
					},
					"last_month": {
						"title": "Last Months's Power",
						"type": "boolean",
						"default": false
					},
					"last_year": {
						"title": "Last Year's Power",
						"type": "boolean",
						"default": false
					},
					"life_time": {
						"title": "Lifetime Power",
						"type": "boolean",
						"default": false
					}
				}
			}
		}
	}
}   

